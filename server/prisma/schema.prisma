generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model post {
  id       Int    @id @default(autoincrement())
  nombre   String
  apellido String
  edad     Int
}

model usuarios {
  id_usuarios      Int     @id @default(autoincrement())
  nombre_usuario   String? @db.VarChar(50)
  apellido_usuario String? @db.VarChar(50)
  correo           String? @db.VarChar(50)
  contrasenia      String? @db.VarChar(50)
}
